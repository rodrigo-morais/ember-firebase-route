<section>
    {{#each spent in model}}
        {{#link-to 'spent' spent}}
          {{partial "spender"}}
          {{view "spender" name=spent.item}}
        {{/link-to}}
        <div class="spending">
          <div>Date: {{spent.date}}</div>
          <div>Value: US$ {{spent.value}}</div>
        </div>
    {{/each}}
</section>